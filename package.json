{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:vue": "vue-cli-service serve",
    "build:vue": "vue-cli-service build",
    "serve:jekyll": "bundle exec jekyll build --watch",
    "build:jekyll": "bundle exec jekyll build",
    "move": "node removeFiles",
    "serve": "run-p serve:jekyll serve:vue",
    "build": "run-s build:vue move build:jekyll"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "fs-extra": "^5.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
